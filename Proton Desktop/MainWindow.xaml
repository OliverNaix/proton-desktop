<Window x:Class="Proton_Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proton_Desktop"
        mc:Ignorable="d"
        IsTabStop="False"
        AllowsTransparency="True"
        Title="MainWindow" 
        Background="#FFF"
        Height="450" Width="800" Loaded="Window_Loaded" WindowStyle="None" ResizeMode="CanResizeWithGrip">
    <Window.Effect>
        <DropShadowEffect/>
    </Window.Effect>
    <Grid Visibility="Visible">

        <Grid  Background="#f1f1f1"
              Height="20"
              VerticalAlignment="Top">
            <Button Width="25" Height="Auto" HorizontalAlignment="Right" Margin="0,0,10,0"/>
        </Grid>
        <Frame x:Name="Frame" NavigationUIVisibility="Hidden" Margin="0,20,0,0" IsTabStop="False"/>

    </Grid>
</Window>
